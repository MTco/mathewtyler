SetEnvIfNoCase X-Forwarded-For .+ proxy=yes
SetEnvIfNoCase X-moz prefetch no_access=yes

<Files ~ "\.(htaccess|wmv|exe|zip|mp3|mp4|mpg|mpeg|avi|xvid)$">
	order allow,deny
	deny from all
</Files>

ErrorDocument 404 /errors/index.php?error_code=404
ErrorDocument 403 /errors/index.php?error_code=403
ErrorDocument 500 /errors/index.php?error_code=500

<IfModule mod_expires.c>
	ExpiresActive On
	ExpiresDefault A4838400
	ExpiresByType image/gif A4838400
	ExpiresByType image/png A4838400
	ExpiresByType image/jpg A4838400
	ExpiresByType image/x-icon A4838400
	ExpiresByType application/x-javascript A4838400
	ExpiresByType text/plain A4838400
	ExpiresByType text/css A4838400
</IfModule>

<ifmodule mod_php4.c>
	php_flag zlib.output_compression on
	php_value zlib.output_compression 16386
	php_value zlib.output_compression_level 6
</ifmodule>

IndexIgnore *.js *.swf *.exe *.rar *.zip
