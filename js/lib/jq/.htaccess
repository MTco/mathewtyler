Options +FollowSymLinks -Indexes -ExecCGI
DirectoryIndex index.php index.html /index.php

RewriteEngine On
DirectoryIndex index.php index.asp index.html index.htm
ServerSignature Off
LimitRequestBody 10240000

AddDefaultCharset UTF-8
DefaultLanguage en-US

AddHandler cgi-script .pl .py .jsp. .shtml .sh .asp .cgi

SetEnv SITE_WEBMASTER "Mathew Tyler"
SetEnv SITE_WEBMASTER_URI http://mathewtyler.co/contact.html

#ForceType application/x-httpd-php
#RemoveHandler .html .htm
#AddType application/x-httpd-php5 .html .php
#AddType application/x-httpd-php .html .htm
#.htm server parsed .html server parsed
#AddType x-httpd-php .html .htm
#Addhandler application/x-httpd-php5 .html .php
#Addhandler application/x-httpd-php .htm .html

AddType video/x-flv .flv
AddType application/x-shockwave-flash .swf
AddType image/x-icon .ico
AddType application/rss+xml .rss .xml
AddType application/octet-stream .pdf
AddType application/octet-stream 262-PJPTT0A.html

#SSLOptions +StrictRequire
#SSLRequireSSL
#SSLRequire %{HTTP_HOST} eq "mathewtyler.co"
